<templateSet group="Ais">
  <template name="aisload" value="com.nextzy.myais.common.widget.dialog.AisDialog.getInstance().showLoadingDialog($fragment$);" description="show loading dialog" toReformat="true" toShortenFQNames="true">
    <variable name="fragment" expression="enum(&quot;getFragmentManager()&quot;,&quot;getSupportFragmentManager()&quot;)" defaultValue="getFragmentManager()" alwaysStopAt="true" />
    <context>
      <option name="JAVA_STATEMENT" value="true" />
      <option name="JAVA_EXPRESSION" value="true" />
    </context>
  </template>
  <template name="Mobile" value="com.nextzy.myais.common.config.mobile.Mobile.getInstance().$method$" description="get Mobile Singleton" toReformat="true" toShortenFQNames="true">
    <variable name="method" expression="complete()" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_STATEMENT" value="true" />
      <option name="JAVA_EXPRESSION" value="true" />
    </context>
  </template>
  <template name="rxregis" value="com.hwangjr.rxbus.RxBus.get().register(this);" toReformat="true" toShortenFQNames="true">
    <context>
      <option name="JAVA_STATEMENT" value="true" />
      <option name="JAVA_EXPRESSION" value="true" />
    </context>
  </template>
  <template name="rxunregis" value="com.hwangjr.rxbus.RxBus.get().unregister(this);" toReformat="true" toShortenFQNames="true">
    <context>
      <option name="JSON" value="false" />
      <option name="JAVA_STATEMENT" value="true" />
      <option name="JAVA_EXPRESSION" value="true" />
    </context>
  </template>
  <template name="AisTextView" value=" &lt;com.nextzy.myais.common.widget.AISTextView&#10;        style=&quot;@style/AISTextView.Detail.$color$&quot;&#10;        android:layout_width=&quot;match_parent&quot;&#10;        android:layout_height=&quot;wrap_content&quot;&#10;        android:gravity=&quot;center&quot;&#10;        tools:text=&quot;@string/loading&quot;&#10;        /&gt;" description="xml for AisTextView" toReformat="true" toShortenFQNames="true">
    <variable name="color" expression="complete()" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="XSL_TEXT" value="true" />
      <option name="XML" value="true" />
      <option name="XML_TEXT" value="true" />
    </context>
  </template>
  <template name="aisdiss" value="com.nextzy.myais.common.widget.dialog.AisDialog.getInstance().dismissDialog($fragment$);" description="show loading dialog" toReformat="true" toShortenFQNames="true">
    <variable name="fragment" expression="enum(&quot;getFragmentManager()&quot;,&quot;getSupportFragmentManager()&quot;)" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_STATEMENT" value="true" />
      <option name="JAVA_EXPRESSION" value="true" />
    </context>
  </template>
  <template name="idmain" value="android:id=&quot;@+id/promotion_$submodule$_$view_short_hand$_$description$&quot;" description="name id for ais tab main android" toReformat="true" toShortenFQNames="true">
    <variable name="submodule" expression="enum(&quot;currentpackage&quot;,&quot;changepackage&quot;)" defaultValue="" alwaysStopAt="true" />
    <variable name="view_short_hand" expression="enum(&quot;tv&quot;,&quot;btn&quot;,&quot;layout&quot;)" defaultValue="" alwaysStopAt="true" />
    <variable name="description" expression="enum(&quot;username&quot;,&quot;label&quot;)" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="XSL_TEXT" value="true" />
      <option name="XML" value="true" />
      <option name="XML_TEXT" value="true" />
    </context>
  </template>
</templateSet>